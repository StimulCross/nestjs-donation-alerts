		"rxjs": "*"
	},
	"devDependencies": {
		"@donation-alerts/api": "^3.0.1",
		"@donation-alerts/auth": "^3.0.1",
		"@nestjs/common": "^10.3.10",
		"@nestjs/core": "^10.3.10",
		"reflect-metadata": "^0.2.2",
		"rimraf": "^5.0.9",
		"rxjs": "^7.8.1"
